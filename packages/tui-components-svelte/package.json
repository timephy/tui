{
    "name": "@timephy/tui-components-svelte",
    "version": "0.0.6",
    "description": "",
    "homepage": "https://github.com/timephy/tui/tree/main/tui-components-svelte#readme",
    "scripts": {
        "clean": "rm -rf .svelte-kit/ build/ dist/",
        "build": "vite build && svelte-package",
        "lint": "pnpm run check && prettier --check . && publint",
        "dev": "vite dev",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write ."
    },
    "nx": {
        "targets": {
            "build": {
                "inputs": [
                    "default",
                    "!{projectRoot}/.svelte-kit/",
                    "!{projectRoot}/build/",
                    "!{projectRoot}/dist/"
                ],
                "outputs": [
                    "{projectRoot}/.svelte-kit/",
                    "{projectRoot}/build/",
                    "{projectRoot}/dist/"
                ]
            }
        },
        "includedScripts": [
            "clean",
            "build",
            "lint"
        ]
    },
    "files": [
        "dist/"
    ],
    "exports": {
        "./localStorage": "./dist/localStorage/index.svelte.js",
        "./call": "./dist/call/index.js",
        "./call/CallControls": {
            "types": "./dist/call/ui/CallControls.svelte.d.ts",
            "svelte": "./dist/call/ui/CallControls.svelte"
        },
        "./call/Stats": {
            "types": "./dist/call/ui/Stats.svelte.d.ts",
            "svelte": "./dist/call/ui/Stats.svelte"
        },
        "./call/Video": {
            "types": "./dist/call/ui/Video.svelte.d.ts",
            "svelte": "./dist/call/ui/Video.svelte"
        },
        "./media/MediaControls": {
            "types": "./dist/media/ui/MediaControls.svelte.d.ts",
            "svelte": "./dist/media/ui/MediaControls.svelte"
        },
        "./media/MediaState": {
            "types": "./dist/media/ui/MediaState.svelte.d.ts",
            "svelte": "./dist/media/ui/MediaState.svelte"
        },
        "./media/MediaSettingsSelect": {
            "types": "./dist/media/ui/settings/MediaSettingsSelect.svelte.d.ts",
            "svelte": "./dist/media/ui/settings/MediaSettingsSelect.svelte"
        },
        "./media/MediaSettingsVolume": {
            "types": "./dist/media/ui/settings/MediaSettingsVolume.svelte.d.ts",
            "svelte": "./dist/media/ui/settings/MediaSettingsVolume.svelte"
        },
        "./Select": {
            "types": "./dist/ui/Select.svelte.d.ts",
            "svelte": "./dist/ui/Select.svelte"
        },
        "./Slider": {
            "types": "./dist/ui/Slider.svelte.d.ts",
            "svelte": "./dist/ui/Slider.svelte"
        },
        "./Switch": {
            "types": "./dist/ui/Switch.svelte.d.ts",
            "svelte": "./dist/ui/Switch.svelte"
        },
        "./SwitchLabel": {
            "types": "./dist/ui/SwitchLabel.svelte.d.ts",
            "svelte": "./dist/ui/SwitchLabel.svelte"
        }
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@sveltejs/adapter-node": "^5.2.0",
        "@sveltejs/adapter-static": "^3.0.1",
        "@sveltejs/kit": "^2.5.18",
        "@sveltejs/package": "^2.3.1",
        "@sveltejs/vite-plugin-svelte": "4.0.0-next.4",
        "@types/eslint": "^8.56.0",
        "@types/express": "^4.17.21",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "autoprefixer": "^10.4.19",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.36.0-next.4",
        "postcss": "^8.4.38",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "svelte-check": "^3.8.5",
        "tailwindcss": "^3.4.3",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.3.4"
    },
    "dependencies": {
        "@timephy/rnnoise-wasm": "^1.0.0",
        "@timephy/tui-colors-tw": "workspace:*",
        "@timephy/tui-components-tw": "workspace:*",
        "@timephy/tui-icons-svelte": "workspace:*",
        "express": "^4.19.2",
        "rxjs": "^7.8.1",
        "socket.io": "^4.7.5",
        "socket.io-client": "^4.7.5",
        "svelte": "5.0.0-next.178"
    },
    "type": "module",
    "publishConfig": {
        "access": "public"
    }
}
