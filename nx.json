{
    "installation": {
        "version": "19.0.4",
        "plugins": {
            "@nx/vite": "19.0.4"
        }
    },
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "targetDefaults": {
        "clean": {
            "dependsOn": [],
            "cache": false
        },
        "codegen": {
            "dependsOn": [],
            "cache": true
        },
        "build": {
            "dependsOn": ["codegen", "^build"],
            "cache": true
        },
        "lint": {
            "dependsOn": ["build"],
            "inputs": ["default"],
            "cache": true
        }
    },
    "release": {
        "projects": ["packages/*"]
    }
}
